<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" version="2.0">

  <entity class="com.lockerz.meatshop.model2.Address" access="FIELD">
    <table name="address"/>
    <attributes>
      <id name="_id">
        <column name="id"/>
        <generated-value strategy="IDENTITY"/>
      </id>
      <basic name="_street">
        <column name="street"/>
      </basic>
      <basic name="_city">
        <column name="city"/>
      </basic>
    </attributes>
  </entity>

</entity-mappings>
